#include <stdint.h>
#include "main.h"


int main(void)
{

	RCC_AHB1ENR_t volatile *const pClkControl = ADDR_AHB1ENR_REG;
	//Enable clock for GPIO D
	pClkControl->gpiod_en = CLOCK_ENABLE;

	GPIOx_MODE_t volatile *const pGpioDMode = ADDR_GPIOD_MODE_REG;
	//Set GPIO D pin 12 to general purpose output mode
	pGpioDMode->pin_12 = OUTPUT_MODE;


    GPIOx_ODR_t volatile *const pGpioDOutputData = ADDR_GPIOD_OD_REG;


    //toggle led
    while(1)
    {
    //turns on gpio d 12 led
    pGpioDOutputData->pin_12=PIN_STATE_HIGH;

    //delay
    for(uint32_t i=0; i < 100000; i++);

    //turn off led
    pGpioDOutputData->pin_12=PIN_STATE_LOW;

    //delay
    for(uint32_t i=0; i < 100000; i++);
    }
}
